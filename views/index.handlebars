    <!-- Modal for logging in -->
    <div id="login_modal" class="modal">
      <div class="modal-content">
        <form>
          <div class="row">
            <div class="container">
              <div class="input-field col s6">
                <input placeholder="username" id="user_name" type="text" class="validate">
                <label for="user_name ">User Name</label>
              </div>
            </div>
            <div class="container">
              <div class="input-field col s6">
                <input placeholder="********" id="user_password" type="password" class="validate">
                <label for="user_password">Password</label>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="center col s12">
              <button class="btn waves-effect waves-light blue accent-4" id="user_login_submit" type="submit " name="action ">Log In
              </button>
            </div>
          </div>
        </form>
      </div>
    </div>


    <!-- Modal for registering -->
    <div id="register_modal" class="modal">
      <div class="modal-content">
        <form>
          <div class="row">
            <div class="container">
              <div class="input-field col s6">
                <input placeholder="username" id="new_user_name" type="text" class="validate">
                <label for="user_name ">Desired user name:</label>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="container">
              <div class="input-field col s6">
                <input placeholder="********" id="new_user_password" type="password" class="validate">
                <label for="user_password">Desired password:</label>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="container">
              <div class="input-field col s6">
                <input placeholder="jane.doe@example.com" id="new_user_email" type="email" class="validate">
                <label for="user_email">Email:</label>
              </div>
            </div>
          </div>
      </div>
      <div class="row">
        <div class="center col s12">
          <button class="btn waves-effect waves-light blue accent-4" id="user_register_submit" type="submit" name="action">Register
          </button>
        </div>
      </div>
      </form>
    </div>
    </div>

{{!-- modal for posting --}}

<div id="new_post_modal" class="modal">
      <div class="modal-content">
        <form>
          <div class="row">
            <div class="container">
              <div class="input-field col s6">
                <input placeholder="Title" id="new_post_title" type="text" class="validate">
                <label for="new_post_title">Title</label>
              </div>
            </div>
            <div class="container">
              <div class="input-field col s6">
                <textarea placeholder="Post" id="new_post_body" type="text" class="materialize-textarea validate"></textarea>
                <label for="new_post_body">Post</label>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="center col s12">
              <button class="btn waves-effect waves-light blue accent-4" id="new_post__submit" type="submit " name="action ">Post
              </button>
            </div>
          </div>
        </form>
      </div>
    </div>

